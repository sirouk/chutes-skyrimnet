# Chutes CLI / private chute deployment configuration
# Copy to `.env` and export before running scripts.

# Shared username for all deploy_*.py chutes
CHUTES_USERNAME=your_chutes_username

# XTTS specific overrides
XTTS_MODEL_ID=tts_models/multilingual/multi-dataset/xtts_v2
XTTS_WHISPER_MODEL=large-v3-turbo

# VibeVoice overrides
VIBEVOICE_MODEL_ID=microsoft/VibeVoice-1.5B
VIBEVOICE_WHISPER_MODEL=large-v3-turbo

# Higgs overrides
HIGGS_MODEL_ID=bosonai/higgs-audio-v2-generation-3B-base
HIGGS_AUDIO_TOKENIZER=bosonai/higgs-audio-v2-tokenizer
HIGGS_WHISPER_MODEL=large-v3-turbo

# Zonos overrides
ZONOS_MODEL_ID=Zyphra/Zonos-v0.1-hybrid
ZONOS_WHISPER_MODEL=large-v3-turbo

# Global Whisper fallback (used when per-model vars unset)
WHISPER_MODEL=large-v3-turbo

